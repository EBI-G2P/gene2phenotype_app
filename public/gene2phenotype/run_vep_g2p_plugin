perl $HOME/src/ensembl-vep/vep \
--offline \
--assembly GRCh38 \
--cache \
--fasta Homo_sapiens.GRCh38.dna.primary_assembly.fa.gz \
--force_overwrite \
--input_file ${input_dir}/input.vcf \
--output_file ${output_dir}/output.txt \
--plugin G2P,file=DDG2P.csv,txt_report=report.txt,html_report=report.html \
