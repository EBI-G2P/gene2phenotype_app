%= include 'header'
%= include 'search'
<div class="container">
 <h3>All restricted G2P entries which are only visible to curators</h3>
  <% foreach my $result (@$gfds_restricted) { %>
    <h3><%= $result->{panel}%></h3>
    <table id="curator_table" class="table">
      <thead>
        <tr>
          <th>Gene symbol</th>
          <th>Disease name</th>
          <th>Go to G2P entry</th>
        </tr>
      </thead>
      <tbody>
        <% foreach my $gfd (@{$result->{gfds}}) { %>
          <tr>
            <td> <%= $gfd->{gene_symbol} %> </td>
            <td> <%= $gfd->{disease_name} %> </td>
            <td>
              <% my $url = '/gene2phenotype/legacy/gfd/?search_type=gfd&dbID=' . $gfd->{GFD_id}; %>
              %= link_to 'Go to G2P entry' => $url
            </td>
          </tr>
        <% } %>
      </tbody>
    </table>
  <% } %>

</div>

%= include 'footer'

